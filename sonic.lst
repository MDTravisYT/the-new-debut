00007294                            ; d5 = Relative X coordinate
00007294                            ; Returns VDP command in d0
00007294                            Calc_VRAM_Pos:
00007294 D86B 0004                  		add.w	4(a3),d4	; Add camera Y coordinate
00007298 DA53                       		add.w	(a3),d5		; Add camera X coordinate
0000729A                            		; Floor the coordinates to the nearest pair of tiles (the size of a block).
0000729A                            		; Also note that this wraps the value to the size of the plane:
0000729A                            		; The plane is 64*8 wide, so wrap at $100, and it's 32*8 tall, so wrap at $200
0000729A 0244 00F0                  		andi.w	#$F0,d4
0000729E 0245 01F0                  		andi.w	#$1F0,d5
000072A2                            		; Transform the adjusted coordinates into a VDP command
000072A2 E94C                       		lsl.w	#4,d4
000072A4 E44D                       		lsr.w	#2,d5
000072A6 D845                       		add.w	d5,d4
000072A8 7003                       		moveq	#3,d0	; Highest bits of plane VRAM address
000072AA 4840                       		swap	d0
000072AC 3004                       		move.w	d4,d0
000072AE 4E75                       		rts	
000072B0                            ; End of function Calc_VRAM_Pos
