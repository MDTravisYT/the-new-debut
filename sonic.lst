0000333C                            NemDec:
0000333C 48E7 FFDE                      movem.l d0-a1/a3-a6,-(sp)
00003340 49F9 00C0 0000                 lea $C00000,a4      ; load VDP Data Port     
00003346 47FA 0000                      lea NemPCD_WriteRowToVDP(pc),a3
0000334A                             
0000334A                            NemDec_Main:
0000334A 43F9 FFFF AA00                 lea $FFFFAA00,a1        ; load Nemesis decompression buffer
00003350 3418                           move.w  (a0)+,d2        ; get number of patterns
00003352 6A00                           bpl.s   @0          ; are we in Mode 0?
00003354 47EB 000A                      lea $A(a3),a3       ; if not, use Mode 1
00003358 E74A                       @0  lsl.w   #3,d2
0000335A 3A42                           movea.w d2,a5
0000335C 7607                           moveq   #7,d3
0000335E 7400                           moveq   #0,d2
00003360 7800                           moveq   #0,d4
00003362 6100 0000                      bsr.w   NemDec_BuildCodeTable
00003366 1A18                           move.b  (a0)+,d5        ; get first byte of compressed data
00003368 E145                           asl.w   #8,d5           ; shift up by a byte
0000336A 1A18                           move.b  (a0)+,d5        ; get second byte of compressed data
0000336C 3C3C 0010                      move.w  #$10,d6         ; set initial shift value
00003370 6100                           bsr.s   NemDec2
00003372 4CDF 7BFF                      movem.l (sp)+,d0-a1/a3-a6
00003376 4E75                           rts
00003378                             
00003378                            ; ---------------------------------------------------------------------------
00003378                            ; Part of the Nemesis decompressor, processes the actual compressed data
00003378                            ; ---------------------------------------------------------------------------
00003378                             
00003378                            NemDec2:
00003378 3E06                           move.w  d6,d7
0000337A 5147                           subq.w  #8,d7           ; get shift value
0000337C 3205                           move.w  d5,d1
0000337E EE69                           lsr.w   d7,d1           ; shift so that high bit of the code is in bit position 7
00003380 0C01 00FC                      cmpi.b  #%11111100,d1       ; are the high 6 bits set?
00003384 6400                           bcc.s   NemDec_InlineData   ; if they are, it signifies inline data
00003386 0241 00FF                      andi.w  #$FF,d1
0000338A D241                           add.w   d1,d1
0000338C 9C31 1000                      sub.b   (a1,d1.w),d6        ; ~~ subtract from shift value so that the next code is read next time around
00003390 0C46 0009                      cmpi.w  #9,d6           ; does a new byte need to be read?
00003394 6400                           bcc.s   @1          ; if not, branch
00003396 5046                           addq.w  #8,d6
00003398 E145                           asl.w   #8,d5
0000339A 1A18                           move.b  (a0)+,d5        ; read next byte
0000339C 1231 1001                  @1  move.b  1(a1,d1.w),d1
000033A0 3001                           move.w  d1,d0
000033A2 0241 000F                      andi.w  #$F,d1          ; get palette index for pixel
000033A6 0240 00F0                      andi.w  #$F0,d0
000033AA                             
000033AA                            NemDec_GetRepeatCount:
000033AA E848                           lsr.w   #4,d0           ; get repeat count
000033AC                             
000033AC                            NemDec_WritePixel:
000033AC E98C                           lsl.l   #4,d4           ; shift up by a nybble
000033AE 8801                           or.b    d1,d4           ; write pixel
000033B0 51CB 0000                      dbf d3,NemDec_WritePixelLoop; ~~
000033B4 4ED3                           jmp (a3)            ; otherwise, write the row to its destination
000033B6                            ; ---------------------------------------------------------------------------
000033B6                             
000033B6                            NemPCD_NewRow:
000033B6 7800                           moveq   #0,d4           ; reset row
000033B8 7607                           moveq   #7,d3           ; reset nybble counter
000033BA                             
000033BA                            NemDec_WritePixelLoop:
000033BA 51C8 FFF0                      dbf d0,NemDec_WritePixel
000033BE 60B8                           bra.s   NemDec2
000033C0                            ; ---------------------------------------------------------------------------
000033C0                             
000033C0                            NemDec_InlineData:
000033C0 5D46                           subq.w  #6,d6           ; 6 bits needed to signal inline data
000033C2 0C46 0009                      cmpi.w  #9,d6
000033C6 6400                           bcc.s   @2
000033C8 5046                           addq.w  #8,d6
000033CA E145                           asl.w   #8,d5
000033CC 1A18                           move.b  (a0)+,d5
000033CE 5F46                       @2  subq.w  #7,d6           ; and 7 bits needed for the inline data itself
000033D0 3205                           move.w  d5,d1
000033D2 EC69                           lsr.w   d6,d1           ; shift so that low bit of the code is in bit position 0
000033D4 3001                           move.w  d1,d0
000033D6 0241 000F                      andi.w  #$F,d1          ; get palette index for pixel
000033DA 0240 0070                      andi.w  #$70,d0         ; high nybble is repeat count for pixel
000033DE 0C46 0009                      cmpi.w  #9,d6
000033E2 64C6                           bcc.s   NemDec_GetRepeatCount
000033E4 5046                           addq.w  #8,d6
000033E6 E145                           asl.w   #8,d5
000033E8 1A18                           move.b  (a0)+,d5
000033EA 60BE                           bra.s   NemDec_GetRepeatCount
