00005A76 3A03                       		move.w  d3,d5					; move the palette entry from the target palette in d5
00005A78 0244 000E                  		andi.w  #$E,d4					; get only red
00005A7C 0245 000E                  		andi.w	#$E,d5					; get only red
00005A80 B845                       		cmp.w	d5,d4					; compare the two colors
00005A82 6700                       		beq.s	@nextpalette			; if red is equal, skip
00005A84 6200                       		bhi.s	@decred					; if red is higher, branch
