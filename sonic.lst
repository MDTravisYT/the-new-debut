00003486 B982                           eor.l   d4,d2           ; XOR the previous row by the current row
00003488 2882                           move.l  d2,(a4)         ; and write the result
0000348A 534D                           subq.w  #1,a5
0000348C 380D                           move.w  a5,d4
0000348E 66B6                           bne.s   NemPCD_NewRow
00003490 4E75                           rts
