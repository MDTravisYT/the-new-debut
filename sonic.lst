00003400                            ; ---------------------------------------------------------------------------
00003400                            ; Part of the Nemesis decompressor, processes the actual compressed data
00003400                            ; ---------------------------------------------------------------------------
00003400                             
00003400                            NemDec2:
00003400 3E06                           move.w  d6,d7
00003402 5147                           subq.w  #8,d7           ; get shift value
00003404 3205                           move.w  d5,d1
00003406 EE69                           lsr.w   d7,d1           ; shift so that high bit of the code is in bit position 7
00003408 0C01 00FC                      cmpi.b  #%11111100,d1       ; are the high 6 bits set?
0000340C 6400                           bcc.s   NemDec_InlineData   ; if they are, it signifies inline data
0000340E 0241 00FF                      andi.w  #$FF,d1
00003412 D241                           add.w   d1,d1
00003414 9C31 1000                      sub.b   (a1,d1.w),d6        ; ~~ subtract from shift value so that the next code is read next time around
00003418 0C46 0009                      cmpi.w  #9,d6           ; does a new byte need to be read?
0000341C 6400                           bcc.s   @1          ; if not, branch
0000341E 5046                           addq.w  #8,d6
00003420 E145                           asl.w   #8,d5
00003422 1A18                           move.b  (a0)+,d5        ; read next byte
00003424 1231 1001                  @1  move.b  1(a1,d1.w),d1
00003428 3001                           move.w  d1,d0
0000342A 0241 000F                      andi.w  #$F,d1          ; get palette index for pixel
0000342E 0240 00F0                      andi.w  #$F0,d0
00003432                             
00003432                            NemDec_GetRepeatCount:
00003432 E848                           lsr.w   #4,d0           ; get repeat count
00003434                             
00003434                            NemDec_WritePixel:
00003434 E98C                           lsl.l   #4,d4           ; shift up by a nybble
00003436 8801                           or.b    d1,d4           ; write pixel
00003438 51CB 0000                      dbf d3,NemDec_WritePixelLoop; ~~
0000343C 4ED3                           jmp (a3)            ; otherwise, write the row to its destination
0000343E                            ; ---------------------------------------------------------------------------
0000343E                             
0000343E                            NemPCD_NewRow:
0000343E 7800                           moveq   #0,d4           ; reset row
00003440 7607                           moveq   #7,d3           ; reset nybble counter
00003442                             
00003442                            NemDec_WritePixelLoop:
00003442 51C8 FFF0                      dbf d0,NemDec_WritePixel
00003446 60B8                           bra.s   NemDec2
00003448                            ; ---------------------------------------------------------------------------
00003448                             
00003448                            NemDec_InlineData:
00003448 5D46                           subq.w  #6,d6           ; 6 bits needed to signal inline data
0000344A 0C46 0009                      cmpi.w  #9,d6
0000344E 6400                           bcc.s   @2
00003450 5046                           addq.w  #8,d6
00003452 E145                           asl.w   #8,d5
00003454 1A18                           move.b  (a0)+,d5
00003456 5F46                       @2  subq.w  #7,d6           ; and 7 bits needed for the inline data itself
00003458 3205                           move.w  d5,d1
0000345A EC69                           lsr.w   d6,d1           ; shift so that low bit of the code is in bit position 0
0000345C 3001                           move.w  d1,d0
0000345E 0241 000F                      andi.w  #$F,d1          ; get palette index for pixel
00003462 0240 0070                      andi.w  #$70,d0         ; high nybble is repeat count for pixel
00003466 0C46 0009                      cmpi.w  #9,d6
0000346A 64C6                           bcc.s   NemDec_GetRepeatCount
0000346C 5046                           addq.w  #8,d6
0000346E E145                           asl.w   #8,d5
00003470 1A18                           move.b  (a0)+,d5
00003472 60BE                           bra.s   NemDec_GetRepeatCount
