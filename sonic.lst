0000148E                            NemDec_ProcessCompressedData:
0000148E 3E06                       		move.w	d6,d7
00001490 5147                       		subq.w	#8,d7	; get shift value
00001492 3205                       		move.w	d5,d1
00001494 EE69                       		lsr.w	d7,d1	; shift so that high bit of the code is in bit position 7
00001496 0C01 00FC                  		cmpi.b	#%11111100,d1	; are the high 6 bits set?
0000149A 6400                       		bcc.s	NemPCD_InlineData	; if they are, it signifies inline data
0000149C 0241 00FF                  		andi.w	#$FF,d1
000014A0 D241                       		add.w	d1,d1
000014A2 1031 1000                  		move.b	(a1,d1.w),d0	; get the length of the code in bits
000014A6 4880                       		ext.w	d0
000014A8 9C40                       		sub.w	d0,d6	; subtract from shift value so that the next code is read next time around
000014AA 0C46 0009                  		cmpi.w	#9,d6	; does a new byte need to be read?
000014AE 6400                       		bcc.s	loc_14B2	; if not, branch
000014B0 5046                       		addq.w	#8,d6
000014B2 E145                       		asl.w	#8,d5
000014B4 1A18                       		move.b	(a0)+,d5	; read next byte
000014B6                            
