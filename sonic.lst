00003452 0C46 0009                      cmpi.w  #9,d6
00003456 6400                           bcc.s   @2
00003458 5046                           addq.w  #8,d6
0000345A E145                           asl.w   #8,d5
0000345C 1A18                           move.b  (a0)+,d5
