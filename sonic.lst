00005A04 7200                       		moveq	#0,d1					; initialize d1. it will be used for the cycle ignore setup
00005A06 7400                       		moveq	#0,d2					; initialize d2. it will be used for comparisons.
00005A08 7600                       		moveq	#0,d3					; initialize d3. it will be used for comparisons.
00005A0A 7800                       		moveq	#0,d4					; initialize d4. it will be used for comparisons.
00005A0C 7A00                       		moveq	#0,d5					; initialize d5. it will be used for comparisons.
00005A0E 7000                       		moveq	#0,d0					; reinitialize d0. it will be used as a flag.
00005A10 7200                       		moveq	#0,d1					; reinitialize d1. it will be used for a loop.
00005A12 2078 FED0                  		movea.l	(p_palreplace).w,a0		; get the RAM pointer. this is the palette the game is using at the moment.
00005A16 2278 FECC                  		movea.l	(p_paltarget).w,a1		; get the ROM pointer. this will be the palette once the replacements are done.
00005A1A 1238 FECA                  		move.b	(v_palcount).w,d1		; get the above water counter. it will be used for loops.
00005A1E 5301                       		subq.b	#1,d1					; subtract 1 from the counter.
