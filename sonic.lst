00005A30                            ; ---------------------------------------------------------------------------
00005A30                            
00005A30                            DynPalette_ColorCheck:
00005A30 3410                       		move.w	(a0),d2					; move the palette entry from the current palette in d2
00005A32 3611                       		move.w	(a1),d3					; move the palette entry from the target palette in d3
00005A34 B642                       		cmp.w	d2,d3					; compare the two values
00005A36 6700 0000                  		beq.w	@nextpalette			; if they are not equal, branch
00005A3A                            
00005A3A 7001                       		moveq	#1,d0					; set the flag that marks that a change has occured.
00005A3C                            
00005A3C                            	; check for blue
00005A3C 3802                       		move.w	d2,d4					; move the palette entry from the current palette in d4
00005A3E 3A03                       		move.w  d3,d5					; move the palette entry from the target palette in d5
00005A40 0244 0E00                  		andi.w  #$E00,d4				; get only blue
00005A44 0245 0E00                  		andi.w	#$E00,d5				; get only blue
00005A48 B845                       		cmp.w	d5,d4					; compare the two colors
00005A4A 6700                       		beq.s	@chkgreen					; if blue is equal, skip
00005A4C 6200                       		bhi.s	@decblue				; if blue is higher, branch
00005A4E                            
00005A4E                            	; blue is lower
00005A4E 0650 0200                  		addi.w	#$200,(a0)				; add some blue to the current color entry
00005A52 6000                       		bra.s   @chkgreen					; go to the next color
00005A54                            
00005A54                            	; blue is higher
00005A54                            	@decblue:
