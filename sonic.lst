00005A5A 3A03                       		move.w  d3,d5					; move the palette entry from the target palette in d5
00005A5C 0244 00E0                  		andi.w  #$E0,d4					; get only green
00005A60 0245 00E0                  		andi.w	#$E0,d5					; get only green
00005A64 B845                       		cmp.w	d5,d4					; compare the two colors
00005A66 6700                       		beq.s	@chkred					; if green is equal, skip
00005A68 6200                       		bhi.s	@decgreen				; if green is higher, branch
00005A6A                            
00005A6A                            	; green is lower
00005A6A 0650 0020                  		addi.w	#$20,(a0)				; add some green to the current color entry
00005A6E 6000                       		bra.s   @chkred					; go to the next color
00005A70                            
00005A70                            	@decgreen:
00005A70 0450 0020                  		subi.w	#$20,(a0)				; subtract some green from the current color entry
00005A74                            
00005A74                            	@chkred:
00005A74 3802                       		move.w	d2,d4					; move the palette entry from the current palette in d4
00005A76 3A03                       		move.w  d3,d5					; move the palette entry from the target palette in d5
00005A78 0244 000E                  		andi.w  #$E,d4					; get only red
00005A7C 0245 000E                  		andi.w	#$E,d5					; get only red
00005A80 B845                       		cmp.w	d5,d4					; compare the two colors
00005A82 6700                       		beq.s	@nextpalette			; if red is equal, skip
00005A84 6200                       		bhi.s	@decred					; if red is higher, branch
00005A86                            
00005A86                            	; red is lower
00005A86 5450                       		addq.w	#$2,(a0)				; add some red to the current color entry
00005A88 6000                       		bra.s   @nextpalette			; go to the next color
00005A8A                            		
00005A8A                            	@decred:
00005A8A 5550                       		subq.w	#$2,(a0)				; subtract some red from the current color entry
00005A8C                            		
