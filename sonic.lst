000033E6 6100 0000                      bsr.w   NemDec_BuildCodeTable
000033EA 1A18                           move.b  (a0)+,d5        ; get first byte of compressed data
000033EC E145                           asl.w   #8,d5           ; shift up by a byte
000033EE 1A18                           move.b  (a0)+,d5        ; get second byte of compressed data
000033F0 3C3C 0010                      move.w  #$10,d6         ; set initial shift value
000033F4 6100                           bsr.s   NemDec2
