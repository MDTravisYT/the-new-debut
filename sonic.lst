00005A7A 4E75                       		rts
00005A7C                            
00005A7C                            ; ---------------------------------------------------------------------------
00005A7C                            ; Go through the above water palette array
00005A7C                            ; ---------------------------------------------------------------------------
00005A7C                            
00005A7C                            DynPalette_Do:
00005A7C 4A38 FECA                  		tst.b	(v_palcount).w			; check that the amount of colors to be changed has actually been set
00005A80 6700                       		beq.s	DynPal_Nochange			; if it was not, branch
00005A82                            
00005A82 7000                       		moveq	#0,d0					; initialize d0. it will be used for the cycle ignore setup
00005A84 7200                       		moveq	#0,d1					; initialize d1. it will be used for the cycle ignore setup
00005A86 7400                       		moveq	#0,d2					; initialize d2. it will be used for comparisons.
00005A88 7600                       		moveq	#0,d3					; initialize d3. it will be used for comparisons.
00005A8A 7800                       		moveq	#0,d4					; initialize d4. it will be used for comparisons.
00005A8C 7A00                       		moveq	#0,d5					; initialize d5. it will be used for comparisons.
00005A8E 7000                       		moveq	#0,d0					; reinitialize d0. it will be used as a flag.
00005A90 7200                       		moveq	#0,d1					; reinitialize d1. it will be used for a loop.
00005A92 2078 FED0                  		movea.l	(p_palreplace).w,a0		; get the RAM pointer. this is the palette the game is using at the moment.
00005A96 2278 FECC                  		movea.l	(p_paltarget).w,a1		; get the ROM pointer. this will be the palette once the replacements are done.
00005A9A 1238 FECA                  		move.b	(v_palcount).w,d1		; get the above water counter. it will be used for loops.
00005A9E 5301                       		subq.b	#1,d1					; subtract 1 from the counter.
00005AA0 6100 0000                  		bsr.w	DynPalette_ColorCheck	; run the code that checks through the arrays.
00005AA4                            
00005AA4 4A00                       		tst.b	d0						; check if d0 is set
00005AA6 6600                       		bne.s	DynPal_Return			; if it is, return
