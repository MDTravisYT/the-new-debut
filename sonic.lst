0000153C                            ; ===========================================================================
0000153C                            
0000153C                            NemBCT_NewPALIndex:
0000153C 3E00                       		move.w	d0,d7
0000153E                            
0000153E                            NemBCT_Loop:
0000153E 1018                       		move.b	(a0)+,d0	; read next byte
00001540 0C00 0080                  		cmpi.b	#$80,d0	; sign bit being set signifies a new palette index
00001544 64EE                       		bcc.s	NemBCT_ChkEnd	; a bmi could have been used instead of a compare and bcc
00001546                            		
00001546 1200                       		move.b	d0,d1
00001548 0247 000F                  		andi.w	#$F,d7	; get palette index
0000154C 0241 0070                  		andi.w	#$70,d1	; get repeat count for palette index
00001550 8E41                       		or.w	d1,d7	; combine the two
00001552 0240 000F                  		andi.w	#$F,d0	; get the length of the code in bits
00001556 1200                       		move.b	d0,d1
00001558 E149                       		lsl.w	#8,d1
0000155A 8E41                       		or.w	d1,d7	; combine with palette index and repeat count to form code table entry
0000155C 7208                       		moveq	#8,d1
0000155E 9240                       		sub.w	d0,d1	; is the code 8 bits long?
00001560 6600                       		bne.s	NemBCT_ShortCode	; if not, a bit of extra processing is needed
00001562 1018                       		move.b	(a0)+,d0	; get code
00001564 D040                       		add.w	d0,d0	; each code gets a word-sized entry in the table
00001566 3387 0000                  		move.w	d7,(a1,d0.w)	; store the entry for the code
0000156A 60D2                       		bra.s	NemBCT_Loop	; repeat
