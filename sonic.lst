00003424                             
00003424                            _ItemLoop:
00003424 1018                           move.b  (a0)+,d0        ; read next byte
00003426 6BF2                           bmi.s   _ChkEnd         ; ~~
00003428 1200                           move.b  d0,d1
0000342A 0247 000F                      andi.w  #$F,d7          ; get palette index
0000342E 0241 0070                      andi.w  #$70,d1         ; get repeat count for palette index
00003432 8E41                           or.w    d1,d7           ; combine the two
00003434 0240 000F                      andi.w  #$F,d0          ; get the length of the code in bits
00003438 1200                           move.b  d0,d1
0000343A E149                           lsl.w   #8,d1
