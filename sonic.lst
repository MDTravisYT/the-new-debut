0000122A                            
0000122A                            ; ||||||||||||||| S U B R O U T I N E |||||||||||||||||||||||||||||||||||||||
0000122A                            
0000122A                            ; Nemesis decompression to VRAM
0000122A                            NemDec:
0000122A 48E7 FFDC                  		movem.l	d0-a1/a3-a5,-(sp)
0000122E 47F9 0000 0000             		lea	(NemPCD_WriteRowToVDP).l,a3	; write all data to the same location
00001234 49F9 00C0 0000             		lea	(vdp_data_port).l,a4	; specifically, to the VDP data port
0000123A 6000                       		bra.s	NemDecMain
