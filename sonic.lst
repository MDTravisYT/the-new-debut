0000346A 0240 0070                      andi.w  #$70,d0         ; high nybble is repeat count for pixel
0000346E 0C46 0009                      cmpi.w  #9,d6
00003472 64C6                           bcc.s   NemDec_GetRepeatCount
00003474 5046                           addq.w  #8,d6
00003476 E145                           asl.w   #8,d5
