000034A8 1018                           move.b  (a0)+,d0        ; read first byte
000034AA                             
000034AA                            _ChkEnd:
000034AA 0C00 00FF                      cmpi.b  #$FF,d0         ; has the end of the code table description been reached?
000034AE 6600                           bne.s   _NewPalIndex        ; if not, branch
