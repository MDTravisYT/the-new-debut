00003782                            Kos_RunBitStream macro
00003782                                dbra    d2,@skip\@
00003782                                moveq   #7,d2                   ; Set repeat count to 8.
00003782                                move.b  d1,d0                   ; Use the remaining 8 bits.
00003782                                not.w   d3                      ; Have all 16 bits been used up?
00003782                                bne.s   @skip\@                   ; Branch if not.
00003782                                move.b  (a0)+,d0                ; Get desc field low-byte.
00003782                                move.b  (a0)+,d1                ; Get desc field hi-byte.
00003782                                if Kos_UseLUT=1
00003782                                move.b  (a4,d0.w),d0            ; Invert bit order...
00003782                                move.b  (a4,d1.w),d1            ; ... for both bytes.
