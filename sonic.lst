00005A72                            ; DynPaletteTransition Lite - by Giovanni.
00005A72                            ; Subroutine that shifts each individual entry of a palette ever so slightly
00005A72                            ; every time it is ran.
00005A72                            ; ---------------------------------------------------------------------------
00005A72                            
00005A72                            DynPaletteTransition:
00005A72 4A38 FEC9                  		tst.b	(v_palflags).w			; check	if any of the flags are set
00005A76 6700                       		beq.s	DynPalette_Return		; if not, return
00005A78                            
00005A78 6100                       		bsr.s	DynPalette_Do
