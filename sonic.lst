000033C4                            ; ------------------------------------------------------------------------------
000033C4                            NemDec:
000033C4 48E7 FFDE                      movem.l d0-a1/a3-a6,-(sp)
000033C8 49F9 00C0 0000                 lea $C00000,a4      ; load VDP Data Port     
000033CE 47FA 0000                      lea NemPCD_WriteRowToVDP(pc),a3
000033D2                             
000033D2                            NemDec_Main:
000033D2 43F9 FFFF AA00                 lea $FFFFAA00,a1        ; load Nemesis decompression buffer
000033D8 3418                           move.w  (a0)+,d2        ; get number of patterns
000033DA 6A00                           bpl.s   @0          ; are we in Mode 0?
000033DC 47EB 000A                      lea $A(a3),a3       ; if not, use Mode 1
000033E0 E74A                       @0  lsl.w   #3,d2
000033E2 3A42                           movea.w d2,a5
000033E4 7607                           moveq   #7,d3
000033E6 7400                           moveq   #0,d2
000033E8 7800                           moveq   #0,d4
000033EA 6100 0000                      bsr.w   NemDec_BuildCodeTable
000033EE 1A18                           move.b  (a0)+,d5        ; get first byte of compressed data
000033F0 E145                           asl.w   #8,d5           ; shift up by a byte
000033F2 1A18                           move.b  (a0)+,d5        ; get second byte of compressed data
000033F4 3C3C 0010                      move.w  #$10,d6         ; set initial shift value
000033F8 6100                           bsr.s   NemDec2
000033FA 4CDF 7BFF                      movem.l (sp)+,d0-a1/a3-a6
000033FE 4E75                           rts
