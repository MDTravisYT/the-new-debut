0000156E E368                       		lsl.w	d1,d0	; get index into code table
00001570 D040                       		add.w	d0,d0	; shift so that high bit is in bit position 7
00001572 7A01                       		moveq	#1,d5
00001574 E36D                       		lsl.w	d1,d5
00001576 5345                       		subq.w	#1,d5	; d5 = 2^d1 - 1
00001578                            
00001578                            NemBCT_ShortCode_Loop:
