000012B6                            
000012B6                            ; ||||||||||||||| S U B	R O U T	I N E |||||||||||||||||||||||||||||||||||||||
000012B6                            
000012B6                            
000012B6                            NemPCD_NewRow:
000012B6 7800                       		moveq	#0,d4	; reset row
000012B8 7608                       		moveq	#8,d3	; reset nybble counter
000012BA                            
000012BA                            NemPCD_WritePixel_Loop:
000012BA 51C8 FFF0                  		dbf	d0,NemPCD_WritePixel
000012BE 60B4                       		bra.s	NemDec_ProcessCompressedData
000012C0                            ; ===========================================================================
000012C0                            
000012C0                            NemPCD_InlineData:
000012C0 5D46                       		subq.w	#6,d6	; 6 bits needed to signal inline data
000012C2 0C46 0009                  		cmpi.w	#9,d6
000012C6 6400                       		bcc.s	loc_14E4
000012C8 5046                       		addq.w	#8,d6
000012CA E145                       		asl.w	#8,d5
000012CC 1A18                       		move.b	(a0)+,d5
000012CE                            
000012CE                            loc_14E4:
000012CE 5F46                       		subq.w	#7,d6	; and 7 bits needed for the inline data itself
000012D0 3205                       		move.w	d5,d1
000012D2 EC69                       		lsr.w	d6,d1	; shift so that low bit of the code is in bit position 0
000012D4 3001                       		move.w	d1,d0
000012D6 0241 000F                  		andi.w	#$F,d1	; get palette index for pixel
000012DA 0240 0070                  		andi.w	#$70,d0	; high nybble is repeat count for pixel
000012DE 0C46 0009                  		cmpi.w	#9,d6
000012E2 64C6                       		bcc.s	NemPCD_ProcessCompressedData
000012E4 5046                       		addq.w	#8,d6
000012E6 E145                       		asl.w	#8,d5
000012E8 1A18                       		move.b	(a0)+,d5
000012EA 60BE                       		bra.s	NemPCD_ProcessCompressedData
