000059AC 7000                       		moveq	#0,d0					; initialize d0. it will be used for the cycle ignore setup
000059AE 7200                       		moveq	#0,d1					; initialize d1. it will be used for the cycle ignore setup
000059B0 7400                       		moveq	#0,d2					; initialize d2. it will be used for comparisons.
000059B2 7600                       		moveq	#0,d3					; initialize d3. it will be used for comparisons.
000059B4 7800                       		moveq	#0,d4					; initialize d4. it will be used for comparisons.
