000014EE 3001                       		move.w	d1,d0
000014F0 0241 000F                  		andi.w	#$F,d1	; get palette index for pixel
000014F4 0240 0070                  		andi.w	#$70,d0	; high nybble is repeat count for pixel
000014F8 0C46 0009                  		cmpi.w	#9,d6
000014FC 64C6                       		bcc.s	NemPCD_ProcessCompressedData
000014FE 5046                       		addq.w	#8,d6
00001500 E145                       		asl.w	#8,d5
