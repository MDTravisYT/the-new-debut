0000345E 0240 0070                      andi.w  #$70,d0         ; high nybble is repeat count for pixel
00003462 0C46 0009                      cmpi.w  #9,d6
00003466 64C6                           bcc.s   NemDec_GetRepeatCount
00003468 5046                           addq.w  #8,d6
0000346A E145                           asl.w   #8,d5
