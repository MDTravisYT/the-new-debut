00001310                            NemPCD_WriteRowToRAM_XOR:
00001310 B982                       		eor.l	d4,d2
00001312 28C2                       		move.l	d2,(a4)+
00001314 534D                       		subq.w	#1,a5
00001316 380D                       		move.w	a5,d4
00001318 66A0                       		bne.s	NemPCD_NewRow
