00005AB0                            ; ---------------------------------------------------------------------------
00005AB0                            
00005AB0                            DynPalette_ColorCheck:
00005AB0 3410                       		move.w	(a0),d2					; move the palette entry from the current palette in d2
00005AB2 3611                       		move.w	(a1),d3					; move the palette entry from the target palette in d3
00005AB4 B642                       		cmp.w	d2,d3					; compare the two values
00005AB6 6700 0000                  		beq.w	@nextpalette			; if they are not equal, branch
00005ABA                            
00005ABA 7001                       		moveq	#1,d0					; set the flag that marks that a change has occured.
00005ABC                            
00005ABC                            	; check for blue
00005ABC 3802                       		move.w	d2,d4					; move the palette entry from the current palette in d4
00005ABE 3A03                       		move.w  d3,d5					; move the palette entry from the target palette in d5
00005AC0 0244 0E00                  		andi.w  #$E00,d4				; get only blue
00005AC4 0245 0E00                  		andi.w	#$E00,d5				; get only blue
00005AC8 B845                       		cmp.w	d5,d4					; compare the two colors
00005ACA 6700                       		beq.s	@chkgreen					; if blue is equal, skip
00005ACC 6200                       		bhi.s	@decblue				; if blue is higher, branch
00005ACE                            
00005ACE                            	; blue is lower
00005ACE 0650 0200                  		addi.w	#$200,(a0)				; add some blue to the current color entry
00005AD2 6000                       		bra.s   @chkgreen					; go to the next color
00005AD4                            
00005AD4                            	; blue is higher
00005AD4                            	@decblue:
00005AD4 0450 0200                  		subi.w	#$200,(a0)				; subtract some blue from the current color entry
00005AD8                            
00005AD8                            	@chkgreen:
00005AD8 3802                       		move.w	d2,d4					; move the palette entry from the current palette in d4
00005ADA 3A03                       		move.w  d3,d5					; move the palette entry from the target palette in d5
00005ADC 0244 00E0                  		andi.w  #$E0,d4					; get only green
00005AE0 0245 00E0                  		andi.w	#$E0,d5					; get only green
00005AE4 B845                       		cmp.w	d5,d4					; compare the two colors
00005AE6 6700                       		beq.s	@chkred					; if green is equal, skip
00005AE8 6200                       		bhi.s	@decgreen				; if green is higher, branch
00005AEA                            
00005AEA                            	; green is lower
00005AEA 0650 0020                  		addi.w	#$20,(a0)				; add some green to the current color entry
00005AEE 6000                       		bra.s   @chkred					; go to the next color
00005AF0                            
00005AF0                            	@decgreen:
00005AF0 0450 0020                  		subi.w	#$20,(a0)				; subtract some green from the current color entry
00005AF4                            
00005AF4                            	@chkred:
00005AF4 3802                       		move.w	d2,d4					; move the palette entry from the current palette in d4
00005AF6 3A03                       		move.w  d3,d5					; move the palette entry from the target palette in d5
00005AF8 0244 000E                  		andi.w  #$E,d4					; get only red
00005AFC 0245 000E                  		andi.w	#$E,d5					; get only red
00005B00 B845                       		cmp.w	d5,d4					; compare the two colors
00005B02 6700                       		beq.s	@nextpalette			; if red is equal, skip
00005B04 6200                       		bhi.s	@decred					; if red is higher, branch
00005B06                            
00005B06                            	; red is lower
00005B06 5450                       		addq.w	#$2,(a0)				; add some red to the current color entry
00005B08 6000                       		bra.s   @nextpalette			; go to the next color
00005B0A                            		
00005B0A                            	@decred:
00005B0A 5550                       		subq.w	#$2,(a0)				; subtract some red from the current color entry
00005B0C                            		
00005B0C                            	@nextpalette:
00005B0C D1FC 0000 0002             		adda.l	#2,a0					; next palette entry
00005B12 D3FC 0000 0002             		adda.l	#2,a1					; next palette entry
00005B18 51C9 FF96                  		dbf		d1,DynPalette_ColorCheck; repeat
00005B1C 4E75                       		rts								; if no colors are left to be checked, return
00005B1C 4E75                       		rts								; if no colors are left to be checked, return
00005B1C 4E75                       		rts								; if no colors are left to be checked, return
