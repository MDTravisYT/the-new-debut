000033CC                            NemDec:
000033CC 48E7 FFDE                      movem.l d0-a1/a3-a6,-(sp)
000033D0 49F9 00C0 0000                 lea $C00000,a4      ; load VDP Data Port     
000033D6 47FA 0000                      lea NemPCD_WriteRowToVDP(pc),a3
