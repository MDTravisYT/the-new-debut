000033CE 43F9 FFFF AA00                 lea $FFFFAA00,a1        ; load Nemesis decompression buffer
000033D4 3418                           move.w  (a0)+,d2        ; get number of patterns
000033D6 6A00                           bpl.s   @0          ; are we in Mode 0?
000033D8 47EB 000A                      lea $A(a3),a3       ; if not, use Mode 1
000033DC E74A                       @0  lsl.w   #3,d2
000033DE 3A42                           movea.w d2,a5
000033E0 7607                           moveq   #7,d3
000033E2 7400                           moveq   #0,d2
000033E4 7800                           moveq   #0,d4
000033E6 6100 0000                      bsr.w   NemDec_BuildCodeTable
000033EA 1A18                           move.b  (a0)+,d5        ; get first byte of compressed data
000033EC E145                           asl.w   #8,d5           ; shift up by a byte
000033EE 1A18                           move.b  (a0)+,d5        ; get second byte of compressed data
000033F0 3C3C 0010                      move.w  #$10,d6         ; set initial shift value
000033F4 6100                           bsr.s   NemDec2
