00007286                            ; Produces a VRAM plane access command from coordinates
00007286                            ; Parameters:
00007286                            ; d4 = Relative Y coordinate
00007286                            ; d5 = Relative X coordinate
00007286                            ; Returns VDP command in d0
00007286                            Calc_VRAM_Pos:
00007286 D86B 0004                  		add.w	4(a3),d4	; Add camera Y coordinate
0000728A DA53                       		add.w	(a3),d5		; Add camera X coordinate
0000728C                            		; Floor the coordinates to the nearest pair of tiles (the size of a block).
0000728C                            		; Also note that this wraps the value to the size of the plane:
0000728C                            		; The plane is 64*8 wide, so wrap at $100, and it's 32*8 tall, so wrap at $200
0000728C 0244 00F0                  		andi.w	#$F0,d4
00007290 0245 01F0                  		andi.w	#$1F0,d5
00007294                            		; Transform the adjusted coordinates into a VDP command
00007294 E94C                       		lsl.w	#4,d4
00007296 E44D                       		lsr.w	#2,d5
00007298 D845                       		add.w	d5,d4
0000729A 7003                       		moveq	#3,d0	; Highest bits of plane VRAM address
0000729C 4840                       		swap	d0
0000729E 3004                       		move.w	d4,d0
000072A0 4E75                       		rts	
000072A2                            ; End of function Calc_VRAM_Pos
