000012DE 0240 0070                  		andi.w	#$70,d0	; high nybble is repeat count for pixel
000012E2 0C46 0009                  		cmpi.w	#9,d6
000012E6 64C6                       		bcc.s	NemPCD_ProcessCompressedData
000012E8 5046                       		addq.w	#8,d6
000012EA E145                       		asl.w	#8,d5
