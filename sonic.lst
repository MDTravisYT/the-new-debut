00005A64 B845                       		cmp.w	d5,d4					; compare the two colors
00005A66 6700                       		beq.s	@chkred					; if green is equal, skip
00005A68 6200                       		bhi.s	@decgreen				; if green is higher, branch
00005A6A                            
