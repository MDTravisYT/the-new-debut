000014B6 1231 1001                  		move.b	1(a1,d1.w),d1
000014BA 3001                       		move.w	d1,d0
000014BC 0241 000F                  		andi.w	#$F,d1	; get palette index for pixel
000014C0 0240 00F0                  		andi.w	#$F0,d0
000014C4                            
000014C4                            NemPCD_ProcessCompressedData:
000014C4 E848                       		lsr.w	#4,d0	; get repeat count
000014C6                            
000014C6                            NemPCD_WritePixel:
000014C6 E98C                       		lsl.l	#4,d4	; shift up by a nybble
000014C8 8801                       		or.b	d1,d4	; write pixel
000014CA 5343                       		subq.w	#1,d3	; has an entire 8-pixel row been written?
000014CC 6600                       		bne.s	NemPCD_WritePixel_Loop	; if not, loop
000014CE 4ED3                       		jmp	(a3)	; otherwise, write the row to its destination, by doing a dynamic jump to NemPCD_WriteRowToVDP, NemDec_WriteAndAdvance, NemPCD_WriteRowToVDP_XOR, or NemDec_WriteAndAdvance_XOR
