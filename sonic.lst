00005A48 B845                       		cmp.w	d5,d4					; compare the two colors
00005A4A 6700                       		beq.s	@chkgreen					; if blue is equal, skip
00005A4C 6200                       		bhi.s	@decblue				; if blue is higher, branch
00005A4E                            
00005A4E                            	; blue is lower
