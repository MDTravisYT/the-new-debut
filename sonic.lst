00005A7A                            ; DynPaletteTransition Lite - by Giovanni.
00005A7A                            ; Subroutine that shifts each individual entry of a palette ever so slightly
00005A7A                            ; every time it is ran.
00005A7A                            ; ---------------------------------------------------------------------------
00005A7A                            
00005A7A                            DynPaletteTransition:
00005A7A 4A38 FEC9                  		tst.b	(v_palflags).w			; check	if any of the flags are set
00005A7E 6700                       		beq.s	DynPalette_Return		; if not, return
00005A80                            
00005A80 6100                       		bsr.s	DynPalette_Do
00005A82                            
00005A82                            	DynPalette_Return:
00005A82 4E75                       		rts
00005A84                            
00005A84                            ; ---------------------------------------------------------------------------
00005A84                            ; Go through the above water palette array
00005A84                            ; ---------------------------------------------------------------------------
00005A84                            
00005A84                            DynPalette_Do:
00005A84 4A38 FECA                  		tst.b	(v_palcount).w			; check that the amount of colors to be changed has actually been set
00005A88 6700                       		beq.s	DynPal_Nochange			; if it was not, branch
00005A8A                            
00005A8A 7000                       		moveq	#0,d0					; initialize d0. it will be used for the cycle ignore setup
00005A8C 7200                       		moveq	#0,d1					; initialize d1. it will be used for the cycle ignore setup
00005A8E 7400                       		moveq	#0,d2					; initialize d2. it will be used for comparisons.
00005A90 7600                       		moveq	#0,d3					; initialize d3. it will be used for comparisons.
00005A92 7800                       		moveq	#0,d4					; initialize d4. it will be used for comparisons.
00005A94 7A00                       		moveq	#0,d5					; initialize d5. it will be used for comparisons.
00005A96 7000                       		moveq	#0,d0					; reinitialize d0. it will be used as a flag.
00005A98 7200                       		moveq	#0,d1					; reinitialize d1. it will be used for a loop.
00005A9A 2078 FED0                  		movea.l	(p_palreplace).w,a0		; get the RAM pointer. this is the palette the game is using at the moment.
00005A9E 2278 FECC                  		movea.l	(p_paltarget).w,a1		; get the ROM pointer. this will be the palette once the replacements are done.
00005AA2 1238 FECA                  		move.b	(v_palcount).w,d1		; get the above water counter. it will be used for loops.
00005AA6 5301                       		subq.b	#1,d1					; subtract 1 from the counter.
00005AA8 6100 0000                  		bsr.w	DynPalette_ColorCheck	; run the code that checks through the arrays.
00005AAC                            
00005AAC 4A00                       		tst.b	d0						; check if d0 is set
00005AAE 6600                       		bne.s	DynPal_Return			; if it is, return
00005AB0                            
00005AB0                            	DynPal_Nochange:
00005AB0 08B8 0000 FEC9             		bclr	#0,(v_palflags).w		; mark the above water palette as no longer in need of changes
00005AB6                            
00005AB6                            	DynPal_Return:
00005AB6 4E75                       		rts
00005AB8                            
00005AB8                            
00005AB8                            ; ---------------------------------------------------------------------------
00005AB8                            ; Go through the array using instructions given by either of the three leading subroutines
00005AB8                            ; ---------------------------------------------------------------------------
00005AB8                            
00005AB8                            DynPalette_ColorCheck:
00005AB8 3410                       		move.w	(a0),d2					; move the palette entry from the current palette in d2
00005ABA 3611                       		move.w	(a1),d3					; move the palette entry from the target palette in d3
00005ABC B642                       		cmp.w	d2,d3					; compare the two values
00005ABE 6700 0000                  		beq.w	@nextpalette			; if they are not equal, branch
00005AC2                            
00005AC2 7001                       		moveq	#1,d0					; set the flag that marks that a change has occured.
00005AC4                            
00005AC4                            	; check for blue
00005AC4 3802                       		move.w	d2,d4					; move the palette entry from the current palette in d4
00005AC6 3A03                       		move.w  d3,d5					; move the palette entry from the target palette in d5
00005AC8 0244 0E00                  		andi.w  #$E00,d4				; get only blue
00005ACC 0245 0E00                  		andi.w	#$E00,d5				; get only blue
00005AD0 B845                       		cmp.w	d5,d4					; compare the two colors
00005AD2 6700                       		beq.s	@chkgreen					; if blue is equal, skip
00005AD4 6200                       		bhi.s	@decblue				; if blue is higher, branch
00005AD6                            
00005AD6                            	; blue is lower
00005AD6 0650 0200                  		addi.w	#$200,(a0)				; add some blue to the current color entry
00005ADA 6000                       		bra.s   @chkgreen					; go to the next color
00005ADC                            
00005ADC                            	; blue is higher
00005ADC                            	@decblue:
00005ADC 0450 0200                  		subi.w	#$200,(a0)				; subtract some blue from the current color entry
00005AE0                            
00005AE0                            	@chkgreen:
00005AE0 3802                       		move.w	d2,d4					; move the palette entry from the current palette in d4
00005AE2 3A03                       		move.w  d3,d5					; move the palette entry from the target palette in d5
00005AE4 0244 00E0                  		andi.w  #$E0,d4					; get only green
00005AE8 0245 00E0                  		andi.w	#$E0,d5					; get only green
00005AEC B845                       		cmp.w	d5,d4					; compare the two colors
00005AEE 6700                       		beq.s	@chkred					; if green is equal, skip
00005AF0 6200                       		bhi.s	@decgreen				; if green is higher, branch
00005AF2                            
00005AF2                            	; green is lower
00005AF2 0650 0020                  		addi.w	#$20,(a0)				; add some green to the current color entry
00005AF6 6000                       		bra.s   @chkred					; go to the next color
00005AF8                            
00005AF8                            	@decgreen:
00005AF8 0450 0020                  		subi.w	#$20,(a0)				; subtract some green from the current color entry
00005AFC                            
00005AFC                            	@chkred:
00005AFC 3802                       		move.w	d2,d4					; move the palette entry from the current palette in d4
00005AFE 3A03                       		move.w  d3,d5					; move the palette entry from the target palette in d5
00005B00 0244 000E                  		andi.w  #$E,d4					; get only red
00005B04 0245 000E                  		andi.w	#$E,d5					; get only red
00005B08 B845                       		cmp.w	d5,d4					; compare the two colors
00005B0A 6700                       		beq.s	@nextpalette			; if red is equal, skip
00005B0C 6200                       		bhi.s	@decred					; if red is higher, branch
00005B0E                            
00005B0E                            	; red is lower
00005B0E 5450                       		addq.w	#$2,(a0)				; add some red to the current color entry
00005B10 6000                       		bra.s   @nextpalette			; go to the next color
00005B12                            		
00005B12                            	@decred:
00005B12 5550                       		subq.w	#$2,(a0)				; subtract some red from the current color entry
00005B14                            		
00005B14                            	@nextpalette:
00005B14 D1FC 0000 0002             		adda.l	#2,a0					; next palette entry
00005B1A D3FC 0000 0002             		adda.l	#2,a1					; next palette entry
00005B20 51C9 FF96                  		dbf		d1,DynPalette_ColorCheck; repeat
00005B24 4E75                       		rts								; if no colors are left to be checked, return
00005B24 4E75                       		rts								; if no colors are left to be checked, return
00005B24 4E75                       		rts								; if no colors are left to be checked, return
