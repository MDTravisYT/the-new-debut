00001246 43F8 AA00                  		lea	(v_ngfx_buffer).w,a1
0000124A 3418                       		move.w	(a0)+,d2	; get number of patterns
0000124C E34A                       		lsl.w	#1,d2
0000124E 6400                       		bcc.s	loc_146A	; branch if the sign bit isn't set
00001250 D6FC 0000                  		adda.w	#NemPCD_WriteRowToVDP_XOR-NemPCD_WriteRowToVDP,a3	; otherwise the file uses XOR mode
00001254                            
00001254                            loc_146A:
