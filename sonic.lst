000012FC 2882                       		move.l	d2,(a4)	; and write the result
000012FE 534D                       		subq.w	#1,a5
00001300 380D                       		move.w	a5,d4
00001302 66B6                       		bne.s	NemPCD_NewRow
00001304 4E75                       		rts	
