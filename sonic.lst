00001274                            ; ---------------------------------------------------------------------------
00001274                            ; Part of the Nemesis decompressor, processes the actual compressed data
00001274                            ; ---------------------------------------------------------------------------
00001274                            
00001274                            ; ||||||||||||||| S U B	R O U T	I N E |||||||||||||||||||||||||||||||||||||||
00001274                            
00001274                            
00001274                            NemDec_ProcessCompressedData:
00001274 3E06                       		move.w	d6,d7
00001276 5147                       		subq.w	#8,d7	; get shift value
00001278 3205                       		move.w	d5,d1
0000127A EE69                       		lsr.w	d7,d1	; shift so that high bit of the code is in bit position 7
0000127C 0C01 00FC                  		cmpi.b	#%11111100,d1	; are the high 6 bits set?
00001280 6400                       		bcc.s	NemPCD_InlineData	; if they are, it signifies inline data
00001282 0241 00FF                  		andi.w	#$FF,d1
00001286 D241                       		add.w	d1,d1
00001288 1031 1000                  		move.b	(a1,d1.w),d0	; get the length of the code in bits
0000128C 4880                       		ext.w	d0
0000128E 9C40                       		sub.w	d0,d6	; subtract from shift value so that the next code is read next time around
00001290 0C46 0009                  		cmpi.w	#9,d6	; does a new byte need to be read?
00001294 6400                       		bcc.s	loc_14B2	; if not, branch
00001296 5046                       		addq.w	#8,d6
00001298 E145                       		asl.w	#8,d5
0000129A 1A18                       		move.b	(a0)+,d5	; read next byte
0000129C                            
0000129C                            loc_14B2:
0000129C 1231 1001                  		move.b	1(a1,d1.w),d1
000012A0 3001                       		move.w	d1,d0
000012A2 0241 000F                  		andi.w	#$F,d1	; get palette index for pixel
000012A6 0240 00F0                  		andi.w	#$F0,d0
000012AA                            
000012AA                            NemPCD_ProcessCompressedData:
000012AA E848                       		lsr.w	#4,d0	; get repeat count
000012AC                            
000012AC                            NemPCD_WritePixel:
000012AC E98C                       		lsl.l	#4,d4	; shift up by a nybble
000012AE 8801                       		or.b	d1,d4	; write pixel
000012B0 5343                       		subq.w	#1,d3	; has an entire 8-pixel row been written?
000012B2 6600                       		bne.s	NemPCD_WritePixel_Loop	; if not, loop
000012B4 4ED3                       		jmp	(a3)	; otherwise, write the row to its destination, by doing a dynamic jump to NemPCD_WriteRowToVDP, NemDec_WriteAndAdvance, NemPCD_WriteRowToVDP_XOR, or NemDec_WriteAndAdvance_XOR
000012B6                            ; End of function NemDec_ProcessCompressedData
