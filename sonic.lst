0000342A 0C00 0080                  		cmpi.b	#$80,d0	; sign bit being set signifies a new palette index
0000342E 64EE                       		bcc.s	NemBCT_ChkEnd	; a bmi could have been used instead of a compare and bcc
00003430                            		
00003430 1200                       		move.b	d0,d1
00003432 0247 000F                  		andi.w	#$F,d7	; get palette index
00003436 0241 0070                  		andi.w	#$70,d1	; get repeat count for palette index
0000343A 8E41                       		or.w	d1,d7	; combine the two
0000343C 0240 000F                  		andi.w	#$F,d0	; get the length of the code in bits
00003440 1200                       		move.b	d0,d1
00003442 E149                       		lsl.w	#8,d1
00003444 8E41                       		or.w	d1,d7	; combine with palette index and repeat count to form code table entry
00003446 7208                       		moveq	#8,d1
00003448 9240                       		sub.w	d0,d1	; is the code 8 bits long?
0000344A 6600                       		bne.s	NemBCT_ShortCode	; if not, a bit of extra processing is needed
0000344C 1018                       		move.b	(a0)+,d0	; get code
0000344E D040                       		add.w	d0,d0	; each code gets a word-sized entry in the table
