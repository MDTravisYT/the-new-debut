00001256 3A42                       		movea.w	d2,a5	; and store it in a5 because there aren't any spare data registers
00001258 7608                       		moveq	#8,d3	; 8 pixels in a pattern row
0000125A 7400                       		moveq	#0,d2
0000125C 7800                       		moveq	#0,d4
0000125E 6100 0000                  		bsr.w	NemDec_BuildCodeTable
00001262 1A18                       		move.b	(a0)+,d5	; get first byte of compressed data
00001264 E145                       		asl.w	#8,d5	; shift up by a byte
00001266 1A18                       		move.b	(a0)+,d5	; get second byte of compressed data
00001268 3C3C 0010                  		move.w	#$10,d6	; set initial shift value
0000126C 6100                       		bsr.s	NemDec_ProcessCompressedData
0000126E 4CDF 3BFF                  		movem.l	(sp)+,d0-a1/a3-a5
