00006F62 E44D                       		lsr.w	#2,d5
00006F64 D845                       		add.w	d5,d4
00006F66 7003                       		moveq	#3,d0	; Highest bits of plane VRAM address
00006F68 4840                       		swap	d0
00006F6A 3004                       		move.w	d4,d0
00006F6C 4E75                       		rts	
