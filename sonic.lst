00003400 3205                           move.w  d5,d1
00003402 EE69                           lsr.w   d7,d1           ; shift so that high bit of the code is in bit position 7
00003404 0C01 00FC                      cmpi.b  #%11111100,d1       ; are the high 6 bits set?
00003408 6400                           bcc.s   NemDec_InlineData   ; if they are, it signifies inline data
0000340A 0241 00FF                      andi.w  #$FF,d1
0000340E D241                           add.w   d1,d1
00003410 9C31 1000                      sub.b   (a1,d1.w),d6        ; ~~ subtract from shift value so that the next code is read next time around
