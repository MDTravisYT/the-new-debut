00005A32 3611                       		move.w	(a1),d3					; move the palette entry from the target palette in d3
00005A34 B642                       		cmp.w	d2,d3					; compare the two values
00005A36 6700 0000                  		beq.w	@nextpalette			; if they are not equal, branch
00005A3A                            
00005A3A 7001                       		moveq	#1,d0					; set the flag that marks that a change has occured.
00005A3C                            
00005A3C                            	; check for blue
00005A3C 3802                       		move.w	d2,d4					; move the palette entry from the current palette in d4
00005A3E 3A03                       		move.w  d3,d5					; move the palette entry from the target palette in d5
00005A40 0244 0E00                  		andi.w  #$E00,d4				; get only blue
